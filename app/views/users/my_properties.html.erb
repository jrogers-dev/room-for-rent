<h2>My Properties</h2>

<% current_user.owned_properties.each do |p| %>
    <%= link_to p.address, property_path(p.id) %>
    <br>
    Rooms: <%= p.rooms %> , Bathrooms: <%= p.bathrooms %>
    <br>
    Size: <%= p.squarefeet %>sqft, Rent: <%= p.rent %>
    <br>
    Tenant: <%= p.tenant_id ? User.find(p.tenant_id).name : "Unoccupied" %>
    <br><hr><br>
<% end %>


<%= button_to "Return to Dashboard", user_root_path, method: "get" %>
<%= button_to "Logout", destroy_user_session_path, method: "delete" %>